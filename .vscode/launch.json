{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "FlowCoverage",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/test_package/build/coverage/bin/FlowCoverage",
        "args": [],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}/test_package",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "Test Build",
        "miDebuggerPath": "/usr/bin/gdb"
      }
    ]
}